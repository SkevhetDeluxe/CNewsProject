@model AppUser

<h3>User: @Model.UserName</h3>
<h4>ID: @Model.Id</h4>

<tr>
    <th>Roles</th>
    <th>Subscription</th>
</tr>
<tr>
    <td></td>
</tr>

<form asp-action="GiveSub">

    <label for="userId">User ID</label>
    <input readonly="readonly" name="userId" value="@Model.Id" />

    <label for="typeId">Type ID</label>
    <input name="typeId" />

    <label for="days">Amount of days</label>
    <input name="days" />

    <button class="btn btn-primary" type="submit">Accept</button>
</form>